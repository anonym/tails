msgid "[[!meta title=\"Tails 5.6 is out\"]]\n"
msgstr "[[!meta title=\"Tails 5.6 est sorti\"]]\n"

#. type: Plain text
#, no-wrap
msgid "[[!meta date=\"Tue, 25 Oct 2022 12:34:56 +0000\"]]\n"
msgstr "[[!meta date=\"Tue, 25 Oct 2022 12:34:56 +0000\"]]\n"

