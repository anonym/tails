#, no-wrap
msgid "[[!inline pages=\"doc/first_steps/start/not_entirely.inline\" raw=\"yes\" sort=\"age\"]]\n"
msgstr "[[!inline pages=\"doc/first_steps/start/not_entirely.inline.pt\" raw=\"yes\" sort=\"age\"]]\n"

